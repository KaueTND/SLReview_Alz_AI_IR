@article{ ISI:000458942500039,
Author = {Lahmiri, Salim and Shmuel, Amir},
Title = {{Detection of Parkinson's disease based on voice patterns ranking and
   optimized support vector machine}},
Journal = {{BIOMEDICAL SIGNAL PROCESSING AND CONTROL}},
Year = {{2019}},
Volume = {{49}},
Pages = {{427-433}},
Month = {{MAR}},
Abstract = {{Parkinson's disease (PD) is a neurodegenerative disorder that causes
   severe motor and cognitive dysfunctions. Several types of physiological
   signals can be analyzed to accurately detect PD by using machine
   learning methods. This work considers the diagnosis of PD based on voice
   patterns. In particular, we focus on assessing the performance of eight
   different pattern ranking techniques (also termed feature selection
   methods) when coupled with nonlinear support vector machine (SVM) to
   distinguish between PD patients and healthy control subjects. The
   parameters of the radial basis function kernel of the SVM classifier
   were optimized by using Bayesian optimization technique. Our results
   show that the receiver operating characteristic and the Wilcoxon-based
   ranking techniques provide the highest sensitivity and specificity. (C)
   2018 Elsevier Ltd. All rights reserved.}},
DOI = {{10.1016/j.bspc.2018.08.029}},
ISSN = {{1746-8094}},
EISSN = {{1746-8108}},
Unique-ID = {{ISI:000458942500039}},
