@inproceedings{ ISI:000477761800016,
Author = {Khosla, Meenakshi and Jamison, Keith and Kuceyeski, Amy and Sabuncu,
   Mert R.},
Editor = {{Stoyanov, D and Taylor, Z and Carneiro, G and SyedaMahmood, T}},
Title = {{3D Convolutional Neural Networks for Classification of Functional
   Connectomes}},
Booktitle = {{DEEP LEARNING IN MEDICAL IMAGE ANALYSIS AND MULTIMODAL LEARNING FOR
   CLINICAL DECISION SUPPORT, DLMIA 2018}},
