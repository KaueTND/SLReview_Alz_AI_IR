@article{ ISI:000467910800007,
Author = {Ge, Chenjie and Qu, Qixun and Gu, Irene Yu-Hua and Jakola, Asgeir Store},
Title = {{Multi-stream multi-scale deep convolutional networks for Alzheimer's
   disease detection using MR images}},
Journal = {{NEUROCOMPUTING}},
Year = {{2019}},
Volume = {{350}},
Pages = {{60-69}},
Month = {{JUL 20}},
Abstract = {{This paper addresses the issue of Alzheimer's disease (AD) detection
   from Magnetic Resonance Images (MRIs). Existing AD detection methods
   rely on global feature learning from the whole brain scans, while
   depending on the tissue types, AD related features in different tissue
   regions, e.g. grey matter (GM), white matter (WM), and cerebrospinal
   fluid (CSF), show different characteristics. In this paper, we propose a
   deep learning method for multi-scale feature learning based on segmented
   tissue areas. A novel deep 3D multi-scale convolutional network scheme
   is proposed to generate multi-resolution features for AD detection. The
   proposed scheme employs several parallel 3D multi-scale convolutional
   networks, each applying to individual tissue regions (GM, WM and CSF)
   followed by feature fusions. The proposed fusion is applied in two
   separate levels: the first level fusion is applied on different scales
   within the same tissue region, and the second level is on different
   tissue regions. To further reduce the dimensions of features and
   mitigate overfitting, a feature boosting and dimension reduction method,
   XGBoost, is utilized before the classification. The proposed deep
   learning scheme has been tested on a moderate open dataset of ADNI (1198
   scans from 337 subjects), with excellent test performance on randomly
   partitioned datasets (best 99.67\%, average 98.29\%), and good test
   performance on subject-separated partitioned datasets (best 94.74\%,
   average 89.51\%). Comparisons with state-of-the-art methods are also
   included. (C) 2019 Elsevier B.V. All rights reserved.}},
DOI = {{10.1016/j.neucom.2019.04.023}},
ISSN = {{0925-2312}},
EISSN = {{1872-8286}},
ResearcherID-Numbers = {{Gu, Irene Y.H./D-4044-2018
   Jakola, Asgeir/J-1738-2018}},
ORCID-Numbers = {{Gu, Irene Y.H./0000-0003-4759-7038
   Jakola, Asgeir/0000-0002-2860-9331}},
