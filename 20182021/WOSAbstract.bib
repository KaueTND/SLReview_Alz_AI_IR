@article{ ISI:000506642000005,
Author = {Gao, Fei and Wu, Teresa and Chu, Xianghua and Yoon, Hyunsoo and Xu,
   Yanzhe and Patel, Bhavika},
Title = {{Deep Residual Inception Encoder-Decoder Network for Medical Imaging
   Synthesis}},
Journal = {{IEEE JOURNAL OF BIOMEDICAL AND HEALTH INFORMATICS}},
Year = {{2020}},
Volume = {{24}},
Number = {{1}},
Pages = {{39-49}},
Month = {{JAN}},
Abstract = {{Image synthesis is a novel solution in precision medicine for scenarios
   where important medical imaging is not otherwise available. The
   convolutional neural network (CNN) is an ideal model for this task
   because of its powerful learning capabilities through the large number
   of layers and trainable parameters. In this research, we propose a new
   architecture of residual inception encoder-decoder neural network
   (RIED-Net) to learn the nonlinear mapping between the input images and
   targeting output images. To evaluate the validity of the proposed
   approach, it is compared with two models from the literature: synthetic
   CT deep convolutional neural network (sCT-DCNN) and shallow CNN, using
   both an institutional mammogram dataset from Mayo Clinic Arizona and a
   public neuroimaging dataset from the Alzheimers Disease Neuroimaging
   Initiative. Experimental results show that the proposed RIED-Net
   outperforms the two models on both datasets significantly in terms of
   structural similarity index, mean absolute percent error, and peak
   signal-to-noise ratio.}},
DOI = {{10.1109/JBHI.2019.2912659}},
ISSN = {{2168-2194}},
EISSN = {{2168-2208}},
ResearcherID-Numbers = {{Yoon, Hyunsoo/ABF-4095-2020
