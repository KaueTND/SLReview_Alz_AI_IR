@inproceedings{10.5555/3370272.3370285,
author = {Guimaraes, Abel AG RB and Abdou, Tamer and Ba\c{s}ar, Ay\c{s}e},
title = {Deep Learning Brain Regions in Harvard-Oxford Atlas Using SegNet Model},
year = {2019},
publisher = {IBM Corp.},
address = {USA},
abstract = {Background: Although the appearance of the brain shows one unique organism, the brain is composed of cognitive regions. Each region can represent some specific function in the brain. The study of the brain's morphology is important to identify changes in the regions and the segmentation process can classify those regions. Several studies conducted on the brain have been using templates, maps and probabilistic models to segment its regions. Method: In this paper, was used a Deep Learning method to segment the Harvard-Oxford atlas as regions of interest. This Atlas presents specific regions that cover the 48 parcellations of the cortical structural areas of the brain. We used a dataset of MRI images from the ADNI 1 database. A collection of 75,402 images was obtained for training and a validation test. Aims: The main goal of this study is to generate an automatic segmentation of the Harvard-Oxford Atlas using SegNet model. Results: it showed a validation DICE of 0.9499, this is a comparative index mostly widely employed with respect to segmentation. Conclusions: The SegNet Model, which was built to segment external scenarios of day by day life, here was able to produce MRI images segmentation, even considering its complexity.},
booktitle = {Proceedings of the 29th Annual International Conference on Computer Science and Software Engineering},
pages = {120–129},
numpages = {10},
keywords = {Harvard-Oxford atlas, convolutional neural network, segnet model, fMRI, segmentation and deep learning, FSL},
location = {Toronto, Ontario, Canada},
series = {CASCON '19}
}

