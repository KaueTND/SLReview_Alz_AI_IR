@article{10.1145/3360498,
author = {Humbert, Mathias and Trubert, Benjamin and Huguenin, K\'{e}vin},
title = {A Survey on Interdependent Privacy},
year = {2019},
issue_date = {January 2020},
publisher = {Association for Computing Machinery},
