@INPROCEEDINGS{7821697,
author={S. Sarraf and G. Tofighi},
booktitle={2016 Future Technologies Conference (FTC)},
title={Deep learning-based pipeline to recognize Alzheimer's disease using fMRI data},
year={2016},
volume={},
