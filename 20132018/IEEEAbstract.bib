@INPROCEEDINGS{8261461,
author={D. Cheng and M. Liu},
booktitle={2017 IEEE International Conference on Imaging Systems and Techniques (IST)},
title={Combining convolutional and recurrent neural networks for Alzheimer's disease diagnosis using PET images},
year={2017},
volume={},
