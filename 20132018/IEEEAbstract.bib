@INPROCEEDINGS{6623471,
author={R. Mahmood and B. Ghimire},
booktitle={2013 20th International Conference on Systems, Signals and Image Processing (IWSSIP)},
title={Automatic detection and classification of Alzheimer's Disease from MRI scans using principal component analysis and artificial neural networks},
year={2013},
volume={},
