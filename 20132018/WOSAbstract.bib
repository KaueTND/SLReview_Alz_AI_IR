Unique-ID = {{ISI:000367443600028}},
}

@article{ ISI:000367493600012,
Author = {Amico, Francesco and Ambrosini, Ettore and Guillem, Francois and Mento,
   Giovanni and Power, Dermot and Pergola, Giulio and Vallesi, Antonino},
Title = {{The Virtual Tray of Objects Task as a novel method to
   electrophysiologically measure visuo-spatial recognition memory}},
Journal = {{INTERNATIONAL JOURNAL OF PSYCHOPHYSIOLOGY}},
Year = {{2015}},
Volume = {{98}},
Number = {{3, 1}},
Pages = {{477-489}},
Month = {{DEC}},
Abstract = {{We explored a novel method to electrophysiologically measure
   visuo-spatial recognition memory using a modified version of the Virtual
   Tray of Objects Task (VTOT). Event-related potentials (ERP) were
   recorded from 18 healthy volunteers during performance in the VTOT.
   Participants were required to detect random repetitions of
   three-dimensional visual stimuli (OLD) and to refrain from responding to
   non-repeated stimuli (NEW). Differences in ERP between the NEW and OLD
   conditions were tested for statistical significance using
   assumption-free non-parametric analyses. Further, a correlation between
   ERP and behavioral measures was sought. Significant OLD-NEW effects were
   found for four ERP components showing distinct spatio-temporal
   characteristics: a posterior positive component appearing at 100 ms
   (P100), a left-lateralized negative component pealcing at approximate to
   250 ms (N250), a frontal negative component at approximate to 300-450 ms
   (FN400), and a right late frontal negativity (rLFN) at approximate to
   500-720 ms. Moreover, individual differences in the OLD-NEW effect
   computed for the rLFN positively correlated with repeated stimulus
   recognition efficiency. However, there were no late left parietal P600
   old/new effects. These findings suggest that the P100 component might
   reflect early visual perception processes taking place during
   performance in the task, whereas the N250 and FN400 components could be
   linked to stimulus-dependent access to visual memory representations and
   familiarity-related processes, respectively. In contrast, we propose
   that the rLFN component could be associated with higher-level cognitive
   functions, such as attention and monitoring processes. Altogether, our
   results suggest that the ERP version of the VTOT could play a role in
   the electrophysiological assessment of visuo-spatial memory and related
   sub-processes. (C) 2015 Elsevier B.V. All rights reserved.}},
DOI = {{10.1016/j.ijpsycho.2015.10.006}},
ISSN = {{0167-8760}},
EISSN = {{1872-7697}},
ResearcherID-Numbers = {{Ambrosini, Ettore/K-5882-2016
   Pergola, Giulio/B-9754-2013
   Vallesi, Antonino/E-8987-2011
